{
    "suggest.autoTrigger": "always",
    "suggest.enablePreview": true,
    "suggest.disableMenu": true,
    "suggest.maxCompleteItemCount": 20,
    "suggest.floatEnable": true,
    "suggest.noselect": false,
    "suggest.snippetIndicator": ">",
    "signature.target": "float",
    "coc.source.around.enable": true,
    "coc.source.buffer.enable": false,
    "coc.source.file.enable": true,
    "coc.preferences.hoverTarget": "float",
    "coc.preferences.enableFloatHighlight": true,
    "diagnostic.refreshOnInsertMode": true,
    "diagnostic.checkCurrentLine": true,
    "diagnostic.refreshAfterSave": true,
    "diagnostic.messageTarget": "float",
    "diagnostic.errorSign": "U+274C",
    "diagnostic.warningSign": ">>",
    "python.jediEnabled": false,
    "python.linting.flake8Path": "/usr/local/bin/flake8",
    "python.formatting.provider": "black",
    "pyls.plugins.pyflakes.enabled": false,
    "python.linting.pylintEnabled": false,
    "pyls.plugins.pycodestyle.enabled": false,
    "python.linting.mypyEnabled": false,
    "python.linting.pep8Enabled": false,
    "python.linting.pydocstyleEnabled": false,
    "python.linting.enabled": false,
    "python.linting.flake8Enabled": true,
    "snippets.ultisnips.directories": [
      "UltiSnips",
      "~/.vim/plugged/vim-go/gosnippets/UltiSnips"
    ],
    "explorer.width":35,
    "explorer.buffer.showHiddenBuffers": true,
    "explorer.file.autoExpandSingleDirectory": true,
    "explorer.activeMode": true,
    "explorer.keyMappings": {
        ">>": "gitStage",
        "<<": "gitUnstage"
    },
    "coc.preferences.extensionUpdateCheck": "daily",
    "clock.enable": false
}
